{
    "name": "ecfectus/container",
    "description": "Lean PSR11 compatible dependency injection container",
    "license": "MIT",
    "homepage": "https://github.com/ecfectus/container",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.5.0",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Ecfectus\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecfectus\\Container\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "docs": "vendor/bin/phpdoc"
    }
}
