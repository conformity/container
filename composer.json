{
    "name": "conformity/container",
    "description": "Lean PSR11 compatible dependency injection container",
    "license": "MIT",
    "homepage": "https://github.com/conformity/container",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Conformity\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conformity\\Container\\Test\\": "tests"
        }
    }
}
